# Increase volume
"amixer set Master 5%+"
   XF86AudioRaiseVolume

# Decrease volume
"amixer set Master 5%-"
   XF86AudioLowerVolume

#MUTE THIS stuff PLEASE
"amixer set Master toggle"
   XF86AudioMute

#IMPORTANT DO NOT CHANGE
"qutebrowser"
   XF86Launch1

# Lock screen
"slock"
   XF86ScreenSaver

# Block computer
"sudo ZZZ"
    XF86Sleep

#Set up other displays without
"lxrandr"
   XF86Display

# Change track
"mocp --next"
   XF86AudioNext

# Previous track
"mocp --previous"
   XF86AudioPrev

# Toggle playing
"mocp --toggle-pause"
   XF86AudioPlay

# Brightness up
"xbacklight +5%"
   XF86MonBrightnessUp

# Brightness down
"xbacklight -5%"
   XF86MonBrightnessDown

# Screenshot
"screenshot"
   Print
"screenshot -s"
   Shift + Print
